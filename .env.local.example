# Local development overrides
# Copy this to .env.local to override settings in .env

# Example: Use a different port
# WP_HOME='http://localhost:8081'

# Example: Enable additional debugging
# WP_DEBUG='true'
# WP_DEBUG_DISPLAY='true'
# WP_DEBUG_LOG='true'

# Example: Use different database credentials
# DB_USER='custom_user'
# DB_PASSWORD='custom_password'

# Example: Configure Stripe test mode
# STRIPE_TEST_PUBLIC_KEY='pk_test_your_key_here'
# STRIPE_TEST_SECRET_KEY='sk_test_your_key_here'
# STRIPE_WEBHOOK_SECRET='whsec_test_your_secret_here'
