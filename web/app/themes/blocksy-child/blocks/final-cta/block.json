{
	"$schema": "https://schemas.wp.org/wp/6.4/block.json",
	"apiVersion": 3,
	"name": "blocksy-child/final-cta",
	"title": "Final CTA",
	"category": "blocksy-landing",
	"icon": "bell",
	"description": "Final call-to-action section",
	"keywords": ["cta", "call", "action", "conversion"],
	"textdomain": "blocksy-child",
	"editorScript": "file:./index.js",
	"style": "file:./style.css",
	"editorStyle": "file:./editor.css",
	"attributes": {
		"headline": {
			"type": "string",
			"default": "Ready to Transform Your Sleep?"
		},
		"description": {
			"type": "string",
			"default": "Join thousands of satisfied customers who have found their perfect mattress"
		},
		"primaryButtonText": {
			"type": "string",
			"default": "Shop Now"
		},
		"primaryButtonUrl": {
			"type": "string",
			"default": "#shop"
		},
		"secondaryButtonText": {
			"type": "string",
			"default": "Learn More"
		},
		"secondaryButtonUrl": {
			"type": "string",
			"default": "#about"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#0f172a"
		},
		"textColor": {
			"type": "string",
			"default": "#ffffff"
		}
	},
	"example": {
		"attributes": {
			"headline": "Ready to Transform Your Sleep?"
		}
	}
}
