{
	"$schema": "https://schemas.wp.org/wp/6.4/block.json",
	"apiVersion": 3,
	"name": "blocksy-child/local-neighborhoods",
	"title": "Local Neighborhoods",
	"category": "blocksy-landing",
	"icon": "location",
