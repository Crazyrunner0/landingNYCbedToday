			"default": [
				{
					"id": 1,
					"name": "Manhattan",
					"description": "All neighborhoods"
				},
				{
					"id": 2,
					"name": "Brooklyn",
					"description": "All neighborhoods"
				},
				{
					"id": 3,
					"name": "Queens",
					"description": "Select areas"
				},
				{
					"id": 4,
					"name": "Bronx",
					"description": "Select areas"
				}
			]
		}
	},
	"example": {
		"attributes": {
			"title": "Serving Your Neighborhood"
		}
	}
}
