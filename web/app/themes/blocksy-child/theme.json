{
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#2563eb",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#64748b",
          "name": "Secondary"
        },
        {
          "slug": "accent",
          "color": "#f59e0b",
          "name": "Accent"
        },
        {
          "slug": "base",
          "color": "#ffffff",
          "name": "Base"
        },
        {
          "slug": "contrast",
          "color": "#0f172a",
          "name": "Contrast"
        },
        {
          "slug": "neutral-100",
          "color": "#f8fafc",
          "name": "Neutral 100"
        },
        {
          "slug": "neutral-200",
          "color": "#e2e8f0",
          "name": "Neutral 200"
        },
        {
          "slug": "neutral-800",
          "color": "#1e293b",
          "name": "Neutral 800"
        }
      ],
      "gradients": [
        {
          "slug": "primary-gradient",
          "gradient": "linear-gradient(135deg, #2563eb 0%, #3b82f6 100%)",
          "name": "Primary Gradient"
        }
      ],
      "duotone": [],
      "custom": true,
      "customGradient": true,
      "link": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "slug": "system",
          "name": "System Font"
        },
        {
          "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
          "slug": "primary",
          "name": "Primary Font"
        },
        {
          "fontFamily": "Georgia, serif",
          "slug": "serif",
          "name": "Serif Font"
        }
      ],
      "fontSizes": [
        {
          "slug": "xs",
          "size": "0.75rem",
          "name": "Extra Small"
        },
        {
          "slug": "sm",
          "size": "0.875rem",
          "name": "Small"
        },
        {
          "slug": "base",
          "size": "1rem",
          "name": "Base"
        },
        {
          "slug": "md",
          "size": "1.125rem",
          "name": "Medium"
        },
        {
          "slug": "lg",
          "size": "1.25rem",
          "name": "Large"
        },
        {
          "slug": "xl",
          "size": "1.5rem",
          "name": "Extra Large"
        },
        {
          "slug": "2xl",
          "size": "1.875rem",
          "name": "2X Large"
        },
        {
          "slug": "3xl",
          "size": "2.25rem",
          "name": "3X Large"
        },
        {
          "slug": "4xl",
          "size": "3rem",
          "name": "4X Large"
        },
        {
          "slug": "5xl",
          "size": "3.75rem",
          "name": "5X Large"
        }
      ],
      "fontWeight": true,
      "lineHeight": true,
      "textTransform": true,
      "letterSpacing": true,
      "customFontSize": true
    },
    "spacing": {
      "units": ["px", "em", "rem", "vh", "vw", "%"],
      "spacingSizes": [
        {
          "slug": "2xs",
          "size": "0.25rem",
          "name": "2X Small"
        },
        {
          "slug": "xs",
          "size": "0.5rem",
          "name": "Extra Small"
        },
        {
          "slug": "sm",
          "size": "0.75rem",
          "name": "Small"
        },
        {
          "slug": "md",
          "size": "1rem",
          "name": "Medium"
        },
        {
          "slug": "lg",
          "size": "1.5rem",
          "name": "Large"
        },
        {
          "slug": "xl",
          "size": "2rem",
          "name": "Extra Large"
        },
        {
          "slug": "2xl",
          "size": "2.5rem",
          "name": "2X Large"
        },
        {
          "slug": "3xl",
          "size": "3rem",
          "name": "3X Large"
        },
        {
          "slug": "4xl",
          "size": "4rem",
          "name": "4X Large"
        },
        {
          "slug": "5xl",
          "size": "5rem",
          "name": "5X Large"
        }
      ],
      "padding": true,
      "margin": true,
      "blockGap": true
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "custom": {
      "transition": "all 0.3s ease",
      "shadow": {
        "none": "none",
        "xs": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
        "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1)",
        "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
        "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
        "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1)",
        "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
      },
      "spacing": {
        "focus-ring": "2px"
      },
      "button": {
        "borderRadius": "0.375rem",
        "transitionDuration": "0.2s"
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--text-primary)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--base)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "padding": {
        "top": "0",
        "right": "var(--wp--preset--spacing--md)",
        "bottom": "0",
        "left": "var(--wp--preset--spacing--md)"
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          }
        },
        ":focus": {
          "outline": "var(--wp--custom--spacing--focus-ring) solid var(--wp--preset--color--primary)"
        }
      },
      "h1": {
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        },
        "typography": {
          "fontSize": "clamp(2rem, 5vw, var(--wp--preset--font-size--5xl))",
          "fontWeight": "700",
          "lineHeight": "1.2"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--xl)",
            "bottom": "var(--wp--preset--spacing--md)"
          }
        }
      },
      "h2": {
        "color": {
          "text": "var(--wp--preset--color--text-primary)"
        },
        "typography": {
          "fontSize": "clamp(1.5rem, 4vw, var(--wp--preset--font-size--3xl))",
          "fontWeight": "700",
          "lineHeight": "1.3"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--lg)",
            "bottom": "var(--wp--preset--spacing--sm)"
          }
        }
      },
      "h3": {
        "color": {
          "text": "var(--wp--preset--color--text-primary)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--2xl)",
          "fontWeight": "700",
          "lineHeight": "1.4"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--lg)",
            "bottom": "var(--wp--preset--spacing--xs)"
          }
        }
      },
      "h4": {
        "color": {
          "text": "var(--wp--preset--color--text-primary)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xl)",
          "fontWeight": "700",
          "lineHeight": "1.4"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--md)",
            "bottom": "var(--wp--preset--spacing--xs)"
          }
        }
      },
      "h5": {
        "color": {
          "text": "var(--wp--preset--color--text-primary)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--lg)",
          "fontWeight": "700",
          "lineHeight": "1.5"
        }
      },
      "h6": {
        "color": {
          "text": "var(--wp--preset--color--text-primary)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--md)",
          "fontWeight": "700",
          "lineHeight": "1.5"
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--base)"
        },
        "border": {
          "radius": "var(--wp--custom--button--borderRadius)",
          "width": "1px",
          "style": "solid",
          "color": "var(--wp--preset--color--primary)"
        },
        "spacing": {
          "padding": {
            "top": "0.625rem",
            "right": "1.25rem",
            "bottom": "0.625rem",
            "left": "1.25rem"
          }
        },
        "typography": {
          "fontWeight": "600",
          "fontSize": "var(--wp--preset--font-size--base)"
        },
        "outline": "none",
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary-dark)"
          },
          "border": {
            "color": "var(--wp--preset--color--primary-dark)"
          }
        },
        ":focus": {
          "outline": "var(--wp--custom--spacing--focus-ring) solid var(--wp--preset--color--primary)"
        },
        ":active": {
          "opacity": "0.85"
        }
      }
    },
    "blocks": {
      "core/paragraph": {
        "color": {
          "text": "var(--wp--preset--color--text-secondary)"
        },
        "typography": {
          "lineHeight": "1.75"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--md)"
          }
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--lg)",
            "bottom": "var(--wp--preset--spacing--md)"
          }
        }
      },
      "core/list": {
        "color": {
          "text": "var(--wp--preset--color--text-secondary)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--md)"
          }
        }
      },
      "core/buttons": {
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--md)",
            "bottom": "var(--wp--preset--spacing--md)"
          }
        }
      }
    }
  },
  "templateParts": [],
  "customTemplates": []
}
